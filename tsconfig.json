{
    "compilerOptions": {
        "outDir": "./built/",
        "baseUrl": "./src",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "experimentalDecorators": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es5",
        "downlevelIteration" : true,
        "lib": [
            "es5",
            "es2015",
            "dom"
        ],
        "strictPropertyInitialization": false
    },
    "include": [
        "./src/**/*"
    ]
}
